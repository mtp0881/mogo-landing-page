//- header -> header-top -> header-content -> header-bottom -> responsive -> menu javascript -> slick slider

header.header
	.container
		.header-top 
			a.header-logo(href="index.html") MoGo
			i.fa.fa-bars.header-toggle
			ul.header-menu
				+menuLink("About","#about")
				+menuLink("Service","#service")
				+menuLink("Work","#work")
				+menuLink("Blog","#blog")
				+menuLink("Contact","#contact")
				li.header-menu-item 
					a.header-menu-icon(href="#")
						i.fa.fa-shopping-cart
				li.header-menu-item 
					a.header-menu-icon(href="#")
						i.fa.fa-search
		.header-content 
			h2.header-content-caption Creative template 
			h1.header-content-main Welcome to mogo 
			a.header-content-more(href="#") Learn more
		.header-bottom
			+headerBottomItem("01","Intro")
			+headerBottomItem("02","Work")
			+headerBottomItem("03","About")
			+headerBottomItem("04","Contact")